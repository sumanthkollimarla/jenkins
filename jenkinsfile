pipeline {
    agent any
    environment {
        ENV_URL = "pipeline.google.com"
        ACCESS_KEY =credentials('AW_ACCESS_KEY')
    }
    stages{
        stage('stage number 1') {
            steps {
                sh "echo One"
                sh "env"
            }
            
        }
        stage('stage number 2'){
            steps {
                 sh "echo One"
                 sh "echo ENV_URL is ${ENV_URL}"
                 sh "echo $ACCESS_KEY"

            }
        }
        stage('stage number 3'){
            environment {
                ENV_URL = "pipeline.google.com"

            }
            steps {
                 sh "echo ENV_URL is $ENV_URL "
                }   
            
            }   
        
        }
    
    }