pipeline {
    agent any
    environment {
        ENV_URL = "pipeline.google.com"
    }
    stages{
        stage('stage number 1') {
            steps {
                echo "One"
            }
            
        }
        stage('stage number 2'){
            steps {
                 sh echo "One"
                 sh "echo ENV_URL is ${ENV_URL}"
            }
        }
        stage('stage number 3'){
            steps {
                 sh '''
                      echo AWS
                      echo DevOps
                      echo Bash
                }   ''' 
            }   
        }
    }